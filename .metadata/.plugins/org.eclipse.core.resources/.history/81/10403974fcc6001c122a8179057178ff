package OOPS

class Car {
	String name;
	String color;
	int model;
	
	def getCarDetails() {
		println "You're driving $name,$model,$color"
	}
	def startEngine() {
		println "Starting engine for $name"
		}
	
	static void main(args) {
		Car toyoto = new Car()
		Car ford = new Car()
		Car mercedes = new Car()
		
		toyoto.name = "Toyato"
		toyoto.model = 2019
		toyoto.color = "Red"
		
		
	}
}
